{
    "version" : "0.2.0",
    "projectName" : "STM32F042K6T",
    "configurations" : [
        {
            "name" : "(OSX) JLink GDB Server",
            "cwd" : "${workspaceFolder}",
            "executable" : "${workspaceFolder}/build/STM32F042K6T.elf",
            "request": "launch",
            "type" : "cortex-debug",
            "servertype" : "external",
            "gdbTarget": "localhost:2331",
            "gdbPath" : "${userHome}/tools/arm-gnu-toolchain-13.2.Rel1-darwin-arm64-arm-none-eabi/bin/arm-none-eabi-gdb",
            "runToEntryPoint": "main",
            "svdFile" : "STM32F0x2.svd",
            "overrideRestartCommands": [
                "monitor halt",
                "monitor reset"
            ],
            "overrideResetCommands": [
                "monitor reset"
            ],
            "preLaunchCommands": [
                "monitor reset"
            ],
        },
